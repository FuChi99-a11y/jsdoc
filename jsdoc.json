{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["app.js"],
        "exclude": ["node_modules"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "destination": "./docs",
        "recurse": true,
        "template": "node_modules/docdash" // Ruta al tema Docdash
    }
}